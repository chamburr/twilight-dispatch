# Logging level
RUST_LOG=info

# Discord bot token
BOT_TOKEN=

# Sharding information
SHARDS_START=0
SHARDS_END=1
SHARDS_TOTAL=2
SHARDS_CONCURRENCY=1
SHARDS_WAIT=6

# Number of clusters
CLUSTERS=2

# Declare default queue
DEFAULT_QUEUE=true

# Resume after a restart
RESUME=true

# Identify payload
INTENTS=32767
LARGE_THRESHOLD=250
STATUS=online
ACTIVITY_TYPE=0
ACTIVITY_NAME=Testing

# Discord channel logs
LOG_CHANNEL=
LOG_GUILD_CHANNEL=

# State configuration
STATE_ENABLED=true
STATE_MEMBER=true
STATE_MEMBER_TTL=60000
STATE_MESSAGE=true
STATE_MESSAGE_TTL=60000
STATE_PRESENCE=true
STATE_EMOJI=true
STATE_VOICE=true
STATE_OLD=false

# RabbitMQ details
RABBIT_HOST=127.0.0.1
RABBIT_PORT=5672
RABBIT_USERNAME=guest
RABBIT_PASSWORD=guest

# Redis address
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=

# Prometheus address
PROMETHEUS_HOST=127.0.0.1
PROMETHEUS_PORT=8005
